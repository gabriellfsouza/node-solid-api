{
  "name": "solid-api",
  "version": "1.0.0",
  "description": "A GymPass style app api.",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "build": "tsup src --outDir build",
    "start": "node build/server.js",
    "migrate": "npx prisma migrate",
    "migrate:dev": "npm run migrate -- dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "compose:up": "docker-compose up -d",
    "compose:start": "docker-compose start",
    "compose:stop": "docker-compose stop",
    "compose:down": "docker-compose down",
    "test": "vitest run --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:watch": "vitest --dir src/use-cases",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "link:vitest-environment-prisma": "cd prisma/vitest-environment-prisma && npm link && cd ../../ && npm link vitest-environment-prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.11.17",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "1.3.0",
    "@vitest/ui": "1.3.0",
    "eslint": "8.56.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.9.1",
    "supertest": "6.3.4",
    "tsup": "8.0.2",
    "tsx": "4.7.1",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/jwt": "8.0.0",
    "@prisma/client": "5.9.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.10",
    "dotenv": "16.4.4",
    "fastify": "4.26.1",
    "zod": "3.22.4"
  }
}
